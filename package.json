{
  "name": "coolify-deploy",
  "version": "0.1.1",
  "description": "A lightweight Infrastructure as Code (IaC) tool for managing and deploying Docker applications to Coolify",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cdeploy": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "tsx src/cli.ts",
    "build": "tsup",
    "clean": "rm -rf dist node_modules",
    "lint": "eslint src/",
    "test": "vitest run",
    "format": "prettier --write ."
  },
  "keywords": [
    "coolify",
    "iac",
    "infrastructure-as-code",
    "deployment",
    "docker",
    "devops"
  ],
  "author": "Julian Stephens",
  "license": "MIT",
  "packageManager": "pnpm@10.18.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/julianstephens/coolify-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/julianstephens/coolify-deploy/issues"
  },
  "homepage": "https://github.com/julianstephens/coolify-deploy#readme",
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "commander": "^14.0.2",
    "pino": "^10.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tsconfig/node-ts": "^23.6.1",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.7"
  }
}
